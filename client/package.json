{
  "name": "react-wp-rest-client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react-app": "^3.1.2",
    "bootstrap": "^5.3.2",
    "dotenv": "^5.0.1",
    "express": "^4.16.4",
    "ignore-styles": "^5.0.1",
    "npm-run-all": "^4.1.5",
    "qs": "^6.6.0",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-helmet": "^5.2.0",
    "react-loadable": "^5.5.0",
    "react-redux": "^5.1.1",
    "react-router-dom": "^5.0.0",
    "react-scripts": "5.0.1",
    "react-transition-group": "^2.5.1",
    "reactstrap": "^9.2.1",
    "redux": "^3.7.2",
    "sass": "^1.69.7",
    "superagent": "^3.8.3",
    "superagent-promise": "^1.1.0"
  },
  "scripts": {
    "build-css": "sass --no-source-map --style=compressed src:src src/styles/scss/styles.scss:src/index.css",
    "watch:sass": "sass --no-source-map -w  src:src",
    "start": "npm-run-all -p serve watch:sass start-js",
    "build": "npm-run-all build-css build-js",
    "start-js": "react-scripts start",
    "serve": "node ssr-init",
    "build-js": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "babel-register": "^6.26.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
